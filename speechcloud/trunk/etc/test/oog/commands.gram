grammar commands;

    public <command> = Command to  <receiver> | payparclip ;

    <receiver> = dispatch {disptach} | 
                 engine  {engine} <number> | 
                 ladder  {ladder}  <number>| 
                 rescue  {rescue} <number> |
                 truck   {truck} <number>;

     <number> = one {1} | two  {2}| three  {3}| four  {4}| five  {5}| six  {6}| seven  {7}| eight  {8}| nine  {9};
